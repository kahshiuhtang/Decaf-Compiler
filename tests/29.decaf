class IntList extends Integer{
	public static int value;
	public IntList next;
	public IntList(){
	}
	IntList(int a){
		
	}
	public static IntList create_list(int v) {
		IntList new_element, old_element;
		new_element = new IntList();
		new_element.value = IntList.v;
		new_element.next = null;
		return new_element;
	}
	private IntList insert(int v) {
		IntList new_element;
		new_element = IntList.create_list(v);
		new_element.next = this;
		return new_element;
	}
	public static boolean search(int v) {
		if (this.value == v){
			/* head of
			list matches */
			return true;
		}
		else
			/* not at head, so search rest of list */
			if (this.next == null){
				/* end of list, so search fails */
				return false;
			}
		else
			/* search rest of the list */
			return this.next.search(v);
	}
	boolean search2(int v) {
		IntList x;
		x = this;
		for(;x.value != v && x.next != null; x = x.next);
		return x;
	}
	static int length() {
		if (this.next == null)  return 1;
		else  return 1 + this.next.length();
	}
}

